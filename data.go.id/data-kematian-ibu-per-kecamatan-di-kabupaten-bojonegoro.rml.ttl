@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#>.
@prefix r2rml: <http://www.w3.org/ns/r2rml#>.
@prefix sdm: <http://purl.org/linked-data/sdmx-dimension#>.
@prefix sd: <http://purl.org/linked-data/sdmx-measure#>.
@prefix cube: <http://purl.org/linked-data/cube#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<#observation2010>
    rml:logicalSource
            [ rml:iterator
               ";";
            rml:referenceFormulation
               ql:CSV;
            rml:source
                "http://data.go.id/dataset/aed431e2-9fb0-499a-b6c2-03457be8c303/resource/6688bd3f-209e-4f8f-8644-405c9034cf6f/download/datakematianibukecamatan20102014.csv" ];
    r2rml:predicateObjectMap
            [ r2rml:objectMap
               [ r2rml:constant "#2010"; r2rml:termType r2rml:IRI ];
            r2rml:predicate
               sdm:refPeriod ],
            [ r2rml:objectMap
               [ rml:reference "2010" ];
            r2rml:predicate
               sd:obsValue ],
            [ r2rml:objectMap
               [ rml:reference "KECAMATAN" ];
            r2rml:predicate
               sdm:refArea ],
            [ r2rml:objectMap
               [ r2rml:constant "#dataset_6688bd3f-209e-4f8f-8644-405c9034cf6f"; r2rml:termType r2rml:IRI ];
            r2rml:predicate
               cube:dataSet ];
    r2rml:subjectMap
       [ r2rml:class cube:Observation; r2rml:template "#o_{@row}_2010" ].
