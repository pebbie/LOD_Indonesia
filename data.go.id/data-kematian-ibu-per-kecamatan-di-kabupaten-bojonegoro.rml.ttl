<#dataset>
    rml:logicalSource
            [ rml:iterator
               "$.result.resources";
            rml:referenceFormulation
               ql:JSONPath;
            rml:source
                "http://data.go.id/api/3/action/package_show?id=data-kematian-ibu-dan-bayi-per-kecamatan-di-kabupaten-bojonegoro" ];
    r2rml:predicateObjectMap
            [ r2rml:objectMap
               [ rml:reference "$.[1].name" ];
            r2rml:predicate
               rdfs:label ];
    r2rml:subjectMap
       [ r2rml:class cube:DataSet; r2rml:template "#dataset_{$.[1].id}" ].
